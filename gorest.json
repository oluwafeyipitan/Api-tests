			"name": "Add new user",
			"item": [
				{
					"name": "Add new user",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var data = pm.response.json()\r",
									"pm.environment.set(\"userID\", data.id);\r",
									"\r",
									"pm.test(\"User created\", function () {\r",
									"    pm.expect(pm.response.code).to.be.oneOf([201, 202]);\r",
									"});\r",
									"pm.test(\"Creation successful\", function () {\r",
									"    pm.response.to.have.status(\"Created\")\r",
									"});\r",
									"pm.test(\"User Id generated\", function () {\r",
									"    pm.expect(pm.response.text()).to.include(\"id\",pm.collectionVariables.get(\"userID\"));\r",
									"});\r",
									"pm.test(\"Name displayed\", function () {\r",
									"    pm.expect(data).to.have.property(\"name\");\r",
									"    pm.expect(data.name).not.eql(null);\r",
									"    pm.expect(data.name).to.eql(\"Takeshi Inui\");\r",
									"});\r",
									"pm.test(\"User status\", function () {\r",
									"    pm.expect(data).to.have.property(\"status\");\r",
									"    pm.expect(data.status).not.eql(null);\r",
									"    pm.expect(data.status).to.eql(\"active\");\r",
									"});\r",
									"pm.test(\"User gender\", function () {\r",
									"    pm.expect(data).to.have.property(\"gender\")\r",
									"    pm.expect(data.gender).not.eql(null)\r",
									"    pm.expect(data.gender).to.eql(\"male\")\r",
									"});\r",
									"pm.test(\"ID should be a number\", function () {\r",
									"    pm.expect(data.id).not.eql(null);\r",
									"    pm.expect(data.id).to.be.a(\"number\");\r",
									"    pm.expect(data.id).be.above(0);\r",
									"});\r",
									"pm.test(\"Name should be a string\", function () {\r",
									"    pm.expect(data.name).to.be.a(\"string\")\r",
									"});\r",
									""
								],
								"type": "text/javascript"
							}
						}
